{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "__COMMENT": "React: Allow JSX in any file types",
    "react/jsx-filename-extension": 0
  }
}